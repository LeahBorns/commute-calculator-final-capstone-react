{
    "name": "commute-calculator-final-capstone-react",
    "version": "1.0.0",
    "description": "Final capstone that will allow bike commuters to calculate the lbs co2 diverted.",
    "main": "js/index.js",
    "directories": {
        "test": "test"
    },
    "scripts": {
        "start": "http-server build",
        "dev": "run-p watch serve",
        "test": "mocha --recursive --compilers js:babel-core/register",
        "react-test": "react-scripts test --env=jsdom",
        "build": "run-s build:*",
        "clean": "rimraf build",
        "serve": "npm start",
        "watch": "run-p watch:*",
        "mkdir": "mkdirp build",
        "clean:html": "rimraf build/index.html",
        "mkdir:html": "run-s mkdir",
        "build:html": "run-s clean:html mkdir:html _build:html",
        "_build:html": "cp index.html build",
        "watch:html": "run-s build:html _watch:html",
        "_watch:html": "chokidar index.html -c \"npm run build:html\"",
        "clean:js": "rimraf build/js",
        "mkdir:js": "mkdirp build/js",
        "build:js": "run-s clean:js mkdir:js _build:js",
        "_build:js": "webpack",
        "watch:js": "run-s build:js _watch:js",
        "_watch:js": "webpack --watch",
        "mkdir:assets": "run-s mkdir",
        "clean:assets": "rimraf build/assets",
        "build:assets": "run-s clean:assets mkdir:assets _build:assets",
        "_build:assets": "cpr assets build/assets",
        "watch:assets": "run-s build:assets _watch:assets",
        "_watch:assets": "chokidar assets/**/* -c \"npm run build:assets\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://leahborns.github.io/commute-calculator-final-capstone-react/build.git"
    },
    "author": "Leah Borns",
    "license": "ISC",
    "bugs": {
        "url": "https://leahborns.github.io/commute-calculator-final-capstone-react/build/issues"
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.20.1",
        "cheerio": "^1.0.0-rc.3",
        "chokidar-cli": "^1.2.3",
        "concurrently": "^3.6.1",
        "core-js": "^2.6.11",
        "cors": "^2.8.5",
        "cpr": "^2.2.0",
        "enzyme": "^3.11.0",
        "enzyme-react-16-adapter-setup": "^0.1.0",
        "express": "^4.18.2",
        "http-server": "^14.1.1",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.29.1",
        "mongodb": "^3.6.2",
        "mongoose": "^5.10.11",
        "morgan": "^1.10.0",
        "npm": "^6.14.8",
        "npm-run-all": "^4.1.5",
        "passport": "^0.3.2",
        "passport-http": "^0.3.0",
        "passport-jwt": "^3.0.0",
        "passport-jwt-strategy": "^1.1.6",
        "react": "^16.14.0",
        "react-addons-test-utils": "^15.6.2",
        "react-dom": "^16.14.0",
        "react-fontawesome": "^1.7.1",
        "react-redux": "^4.4.10",
        "redux": "^3.6.0",
        "redux-thunk": "^2.3.0",
        "rimraf": "^2.7.1",
        "unirest": "^0.5.1"
    },
    "devDependencies": {
        "acorn": "^5.7.4",
        "babel-cli": "^6.11.4",
        "babel-core": "^6.26.3",
        "babel-loader": "^6.2.4",
        "babel-plugin-transform-object-rest-spread": "^6.8.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-es2015": "^6.13.1",
        "babel-preset-react": "^6.11.1",
        "chai": "^3.5.0",
        "chai-enzyme": "^0.5.2",
        "chai-http": "^3.0.0",
        "enzyme-adapter-react-15": "^1.4.2",
        "enzyme-adapter-react-16": "^1.15.5",
        "faker": "^3.1.0",
        "json-loader": "^0.5.7",
        "mkdirp": "^0.5.5",
        "mocha": "^3.2.0",
        "react-scripts": "0.9.0",
        "react-test-renderer": "^15.7.0",
        "sinon": "^1.17.6",
        "sinon-chai": "^2.8.0",
        "webpack": "^1.15.0"
    },
    "homepage": "https://leahborns.github.io/commute-calculator-final-capstone-react/build#readme"
}
